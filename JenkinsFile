pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
				git 'https://github.com/CryptographStudios/HelloWorld'
				bat "\"${tool '15.0'}\\msbuild.exe\" HelloWorld.sln /p:Configuration=Release /p:Platform=\"Any CPU\" /p:ProductVersion=1.0.0.${env.BUILD_NUMBER}"				
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'

            }
        }
    }
}